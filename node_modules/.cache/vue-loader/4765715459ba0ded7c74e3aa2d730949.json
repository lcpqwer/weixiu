\n      <bm-marker :position=\"{lng:125.6292529148,lat:32.2035397816}\" :dragging=\"true\" animation='BMAP_ANIMATION_BOUNCE'>\n        <bm-label content='上海浦东展讯中心1' :labelStyle=\"{color:'red',fontSize:'12px'}\" :offset=\"{width:-35,height:30}\" />\n      </bm-marker>\n      <bm-marker :position=\"{lng:129.6292529148,lat:36.2035397816}\" :dragging=\"true\" animation='BMAP_ANIMATION_BOUNCE'>\n        <bm-label content='上海浦东展讯中心2' :labelStyle=\"{color:'red',fontSize:'12px'}\" :offset=\"{width:-35,height:30}\" />\n      </bm-marker>\n    </baidu-map>\n    <Table :columns=\"columns1\" :data=\"data1\"></Table>\n    <!--省市三级联动-->\n    <div class=\"liandong\">\n      <div class=\"city\" @click=\"toAddress\">{{city}}</div>\n      <v-distpicker type=\"mobile\" @selected='selected' v-show=\"addInp\" style=\"width: 300px\">\n      </v-distpicker>\n      <div class=\"mask\" v-show=\"mask\"></div>\n    </div>\n  </div>\n\n</template>\n\n<script>\n  import VDistpicker from 'v-distpicker'\nexport default {\n  name: 'join_page',\n  components: { VDistpicker },\n  data () {\n    return {\n      columns1: [\n        {\n          type: 'selection',\n          width: 60,\n          align: 'center'\n        },\n        {\n          title: 'Date',\n          key: 'createTime'\n        },\n        {\n          title: 'Name',\n          key: 'name'\n        },\n        {\n          title: '图片对应TypeId',\n          key: 'type'\n        }\n      ],\n      data1:[],\n      city:'请选择地区',\n      addInp :false,\n      mask:false\n    }\n  },\n  created () {\n    this.handleSelectAll()\n  },\n  methods: {\n    handleSelectAll () {\n      this.axios({\n        url: 'Electric/show',\n        method: 'post',\n        // data: $qs.stringify({\n          // Id: isDelete\n        // })\n      }).then(res => {\n        console.log(res,'12345')\n        this.data1 = res.data.table\n\n      }).catch(res => {\n        this.$Message.error('请求超时,请稍后再试...')\n      });\n    },\n// 点击弹出三级联动\n    toAddress(){\n      this.mask = true;\n      this.addInp = true\n    },\n    // 省市区三级联动\n    selected(data){\n      this.mask =false;\n      this.addInp = false;\n      this.city = data.province.value + ' ' + data.city.value +' ' + data.area.value\n    }\n  }\n}\n</script>\n\n<style>\n  .liandong{\n    width: 200rpx;\n  }\n  /* The container of BaiduMap must be set width & height. */\n  .map {\n    width: 100%;\n    height: 200px;\n  }\n\n</style>\n"]}]}